<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="MeuConsultorio.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:MeuConsultorio.Views"
	xmlns:local="clr-namespace:MeuConsultorio">

	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Home"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:MainPage}"
		Route="MainPage" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Blank"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:BlankPage}"
		Route="BlankPage" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="ListDetail"
		Icon="iconlistdetail.png"
		ContentTemplate="{DataTemplate views:ListDetailPage}"
		Route="ListDetailPage" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Acesso"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:AcessoPage}"
		Route="Blank1Page" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Blank2"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:Blank2Page}"
		Route="Blank2Page" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Blank3"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:Blank3Page}"
		Route="Blank3Page" />
</Shell>
